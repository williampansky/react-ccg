{
  "name": "@ccg/game",
  "version": "1.0.0",
  "homepage": "https://react-workspaces.github.io/react-workspaces-playground",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start --scripts-version @react-workspaces/react-scripts",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint ./src/**/*.js --max-warnings=0 --format=codeframe",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ccg/components": "1.0.0",
    "@ccg/config": "1.0.0",
    "@ccg/data": "1.0.0",
    "@ccg/enums": "1.0.0",
    "@ccg/hooks": "1.0.0",
    "@ccg/images": "1.0.0",
    "@ccg/styles": "1.0.0",
    "@ccg/utils": "1.0.0",
    "@react-workspaces/react-scripts": "^3.3.0-alpha-08",
    "@reduxjs/toolkit": "^1.4.0",
    "antd": "^4.5.2",
    "react": "^16.12.0",
    "react-countup": "^4.3.3",
    "react-dom": "^16.12.0",
    "react-image": "^4.0.3",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-svg": "^11.0.34",
    "react-tooltip": "^4.2.8",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.0",
    "boardgame.io": "^0.39.12",
    "cross-env": "6.0.3",
    "customize-cra": "^0.9.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "react-app-rewired": "^2.1.5",
    "source-map-explorer": "^2.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.{js,jsx}",
      "!src/index.{js,jsx}",
      "!src/serviceWorker.{js,jsx}",
      "!src/**/*.(spec|test|stories).{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 98,
        "functions": 98,
        "lines": 98
      }
    }
  }
}
