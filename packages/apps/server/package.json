{
  "name": "@ccg/server",
  "version": "1.0.0",
  "private": true,
  "main": "src/",
  "main:src": "src/index.js",
  "scripts": {
    "start": "npx babel-node --inspect=8000 --ignore=' ' src/server.js",
    "nodemon-extended": "nodemon --exec babel-node --inspect=8000 src/server.js",
    "nodemon-extended-old": "nodemon --exec babel-node --root-mode upward --ignore=' ' -r esm --es-module-specifier-resolution=node --experimental-modules src/server.js",
    "nodemon": "node --experimental-modules -r esm src/server.js"
  },
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "boardgame.io": "^0.39.12",
    "core-js": "3",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "is-promise": "^4.0.0",
    "mathjs": "^7.0.1",
    "nodemon": "^2.0.4",
    "regenerator": "^0.14.4",
    "uuid": "^8.1.0"
  },
  "resolutions": {
    "is-promise": "4.0.0"
  }
